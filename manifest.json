{
    "manifest_version": 3,
    "name": "Example Extension",
    "description": "Explanation on the How to make  Chrome Extensions with normal HTML , CSS and JS and Jquesry",
    "version": "1.0",
    "permissions": [
        "sidePanel",
        "scripting",
        "activeTab",
        "windows"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "bootstrap/jquery-3.2.1.min.js",
                "poplogin.js"
            ]
        }
    ],
    "action": {
        "default_popup": "index.html"
    },
    "side_panel": {
        "default_path": "index.html"
    },
    "icons": {
        "48": "https://files.oaiusercontent.com/file-wjgffFbgQGv53vy8mwzxD4or?se=2024-09-04T10%3A41%3A50Z&sp=r&sv=2024-08-04&sr=b&rscc=max-age%3D604800%2C%20immutable%2C%20private&rscd=attachment%3B%20filename%3D6872cf2e-a46c-4cec-abbf-fd5e5973e6d3.webp&sig=ZXtSkFNldXSl1pxqto/OIbslaE5ZPuVl5NLh/N/WDtY%3D"
    }
}